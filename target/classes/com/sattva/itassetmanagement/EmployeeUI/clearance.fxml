<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="assetPane" prefHeight="647.0" prefWidth="771.0" style="-fx-background-color: #9DB2BF;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sattva.itassetmanagement.EmployeeUI.ClearanceController">
    <children>
        <ImageView fitHeight="83.0" fitWidth="142.0" layoutX="615.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/salarpuria-sattva-logo.png" />
            </image>
        </ImageView>
        <Label alignment="CENTER" layoutY="3.0" prefHeight="100.0" prefWidth="771.0" text="Clearance">
            <font>
                <Font name="Javanese Text" size="36.0" />
            </font>
        </Label>
      <Button fx:id="submitBtn" layoutX="144.0" layoutY="593.0" mnemonicParsing="false" onAction="#onSubmit" prefHeight="40.0" prefWidth="504.0" text="SUBMIT">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TableView fx:id="assetTable" layoutX="38.0" layoutY="138.0" prefHeight="405.0" prefWidth="696.0">
        <columns>
            <TableColumn fx:id="allocationId" maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="AllocationId" />
          <TableColumn fx:id="typeColumn" prefWidth="148.0" text="Type" />
          <TableColumn fx:id="subTypeColumn" prefWidth="175.5" text="SubType" />
            <TableColumn fx:id="serialColumn" prefWidth="159.0" text="Serial" />
            <TableColumn fx:id="dateColumn" prefWidth="212.5" text="Date of Allocation" />
        </columns>
      </TableView>
    </children>
</AnchorPane>
